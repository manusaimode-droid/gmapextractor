{
    "manifest_version": 3,
    "name": "Google Maps Data Extractor",
    "version": "2.0",
    "description": "Professional Side Panel tool to extract business data from Google Maps.",
    "permissions": [
        "scripting",
        "tabs",
        "storage",
        "sidePanel"
    ],
    "host_permissions": [
        "https://www.google.com/*",
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Open Maps Extractor",
        "default_icon": {
            "16": "assets/icons/16.png",
            "48": "assets/icons/48.png",
            "128": "assets/icons/128.png"
        }
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    }
}